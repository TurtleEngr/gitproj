[gitproj "config"]
	local-status = not-defined
	remote-status = not-defined
	proj-name = TBD
	git-flow-pkg = TBD
	remote-min-size = 20g
[gitproj "hook"]
	auto-move = true
	binary-file-size-limit = 2M
	check-file-names = true
	check-for-big-files = true
	pre-commit-enabled = true
	source = hooks/pre-commit
